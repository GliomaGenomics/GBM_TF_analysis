
library(argparse)

parser <- ArgumentParser(description='plot gsea results')
parser$add_argument('--gmt', dest='gmt', type='character', help='gmt file')
args <- parser$parse_args()

lines<-read_lines(args$gmt)


genesets=list()
for(i in lines){
genesets[i[[1]][1]]<-list(i[[1]][3:length(i[[1]])])
}
